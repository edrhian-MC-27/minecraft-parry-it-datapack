{
  "type": "minecraft:confirmation",
  "title": "Parry Creeper Configuration",
  "body": {
    "type": "minecraft:item",
    "item": {
      "id": "minecraft:creeper_head"
    }
  },
  "inputs": [
    {
      "type": "minecraft:number_range",
      "key": "creeper_minimum_fuse_time",
      "label": "Creeper minimum fuse time",
      "start": 0,
      "end": 40,
      "step": 1,
      "initial": 25
    }
  ],
  "can_close_with_escape": true,
  "pause": true,
  "after_action": "close",
  "yes": {
    "label": "Apply Changes",
    "action": {
      "type": "minecraft:dynamic/run_command",
      "template": "scoreboard players set #parry_shield_creeper_minimum_fuse_time .parry_shield_consts $(creeper_minimum_fuse_time)"
    }
  },
  "no": {
    "label": "Discard Changes",
    "action": {
      "type": "minecraft:show_dialog",
      "dialog": "parry_it:dialog_list"
    }
  }
}
